{
  "name": "@growi/codemirror-textlint",
  "version": "6.0.16-RC.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn tsc && tsc-alias -p tsconfig.build.json",
    "clean": "npx -y shx rm -rf dist",
