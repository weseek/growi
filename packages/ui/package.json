{
  "name": "@growi/ui",
  "version": "1.0.0",
  "description": "GROWI UI Libraries",
  "license": "MIT",
  "private": "true",
  "keywords": [
    "growi"
  ],
  "type": "module",
  "files": [
    "dist",
    "scss"
  ],
  "exports": {
    "./dist/components": {
      "import": "./dist/components/index.js"
    },
    "./dist/interfaces": {
      "import": "./dist/interfaces/index.js"
    },
    "./dist/utils": {
      "import": "./dist/utils/index.js"
    },
    "./dist/style": {
      "import": "./dist/style.css"
    },
    "./scss/*": "./scss/*.scss"
  },
  "scripts": {
    "build": "vite build",
    "clean": "shx rm -rf dist",
    "dev": "vite build --mode dev",
    "watch": "yarn dev -w --emptyOutDir=false",
    "lint:js": "yarn eslint **/*.{js,ts}",
    "lint:typecheck": "tsc",
    "lint": "npm-run-all -p lint:*"
  },
  "dependencies": {
    "@growi/core": "link:../core"
  },
  "devDependencies": {
    "date-fns": "^3.6.0",
    "reactstrap": "^9.2.0"
  },
  "peerDependencies": {
    "next": "^14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
