{
  "name": "@growi/remark-drawio",
  "version": "6.0.16-RC.0",
  "description": "remark plugin to draw diagrams with draw.io (diagrams.net)",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown"
  ],
  "main": "dist/index.js",
